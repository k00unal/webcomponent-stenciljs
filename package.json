{
  "name": "webcomponent-stenciljs",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/webcomponent-stenciljs/webcomponent-stenciljs.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "start-stencil": "stencil build --dev --watch",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "story": "npm-run-all --parallel start-stencil storybook",
    "storybook": "start-storybook -p 9001 -c .storybook -s www"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@stencil/core": "^1.0.7",
    "@stencil/postcss": "^1.0.1",
    "@stencil/sass": "^1.0.1",
    "@storybook/addon-viewport": "^5.1.9",
    "@storybook/html": "^5.1.9",
    "@types/autoprefixer": "^9.5.0",
    "@types/jest": "24.0.15",
    "@types/puppeteer": "1.12.4",
    "autoprefixer": "^9.6.0",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.0.3",
    "jest": "24.8.0",
    "jest-cli": "24.8.0",
    "npm-run-all": "^4.1.5",
    "puppeteer": "1.17.0",
    "write-file-webpack-plugin": "^4.5.0"
  },
  "browserslist": [
    "last 2 version",
    "> 2%"
  ],
  "license": "MIT"
}
